{
  "name": "ksa_expo",
  "version": "1.0.0",
  "description": "A live voting app for KSA's K-Expo event. It allows audience members to vote for their favorite performances and ultimately determines the winner. Originally done as a single React + Node app, there were so many problems with the original app that I chose to scrap it and rewrite it entirely so it can be reused year after year.",
  "scripts": {
    "db:migrate-up": "./node_modules/.bin/db-migrate up --config ./db/db.json",
    "db:migrate-down": "./node_modules/.bin/db-migrate down --config ./db/db.json",
    "db:migrate-create": "./node_modules/.bin/db-migrate create --config ./db/db.json",
    "db:reset": "./node_modules/.bin/db-migrate reset --config ./db/db.json",
    "db:create": "./node_modules/.bin/db-migrate db:creat --config ./db/db.json",
    "test": "./node_modules/.bin/mocha './tests/**/*.js'",
    "build": "yarn db:migrate-up && ./node_modules/.bin/gulp",
    "start": "node --harmony ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewgu12/KSA-KExpo.git"
  },
  "author": "Andrew Gu",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/helmet": "^0.0.37",
    "@types/mocha": "^5.2.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.4",
    "@types/pg": "^7.4.9",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "env-var": "^3.2.0",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "mocha": "^5.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pug": "^2.0.3"
  }
}
